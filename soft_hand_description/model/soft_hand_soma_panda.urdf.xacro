<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

	<!-- include other model components -->
	<xacro:include filename="$(find soft_hand_description)/model/materials.urdf.xacro"/>
	<xacro:include filename="$(find soft_hand_description)/model/soft_hand.transmission.xacro"/>
	<xacro:include filename="$(find soft_hand_description)/model/soft_hand.gazebo.xacro"/>
	<xacro:include filename="$(find soft_hand_description)/model/soft_hand.inertia.xacro"/>

	<!-- accesories -->
	<xacro:include filename="$(find soft_hand_description)/model/accesories/panda_flange.urdf.xacro"/>
	<xacro:include filename="$(find soft_hand_description)/model/accesories/softhand_round_base.urdf.xacro"/>

	<!-- joint properties -->
	<xacro:property name="abd_lb" value="-30" />
	<xacro:property name="abd_ub" value="30" />
	<xacro:property name="thumb_abd_lb" value="0" />
	<xacro:property name="thumb_abd_ub" value="90" />
	<xacro:property name="inner_lb" value="0" />
	<xacro:property name="inner_ub" value="45" />
	<xacro:property name="middle_lb" value="0" />
	<xacro:property name="middle_ub" value="45" />
	<xacro:property name="outer_lb" value="0" />
	<xacro:property name="outer_ub" value="45" />
	<xacro:property name="velocity" value="100" />
	<xacro:property name="effort" value="100" />
	<xacro:property name="damping" value="0.0" />
	<xacro:property name="friction" value="0.0" />

	<!-- synergy vector -->
	<xacro:property name="syn_amp" value="1" />
	<xacro:property name="thumb_abd_R" value="${1.3*syn_amp}" />
	<xacro:property name="thumb_inner_R" value="${1.3*syn_amp}" />
	<xacro:property name="thumb_outer_R" value="${1.0*syn_amp}" />
	<xacro:property name="index_abd_R" value="-${0.2*syn_amp}" />
	<xacro:property name="index_inner_R" value="${1.0*syn_amp}" />
	<xacro:property name="index_middle_R" value="${1.0*syn_amp}" />
	<xacro:property name="index_outer_R" value="${1.0*syn_amp}" />
	<xacro:property name="middle_abd_R" value="${0.0*syn_amp}" />
	<xacro:property name="middle_inner_R" value="${1.0*syn_amp}" />
	<xacro:property name="middle_middle_R" value="${1.0*syn_amp}" />
	<xacro:property name="middle_outer_R" value="${1.0*syn_amp}" />
	<xacro:property name="ring_abd_R" value="${0.2*syn_amp}" />
	<xacro:property name="ring_inner_R" value="${1.0*syn_amp}" />
	<xacro:property name="ring_middle_R" value="${1.0*syn_amp}" />
	<xacro:property name="ring_outer_R" value="${1.0*syn_amp}" />
	<xacro:property name="little_abd_R" value="${0.4*syn_amp}" />
	<xacro:property name="little_inner_R" value="${1.0*syn_amp}" />
	<xacro:property name="little_middle_R" value="${1.0*syn_amp}" />
	<xacro:property name="little_outer_R" value="${1.0*syn_amp}" />

	<!-- synergy joint properties -->
	<xacro:property name="synergy_lb" value="0.0" />
	<xacro:property name="synergy_ub" value="1.0"/>
	<xacro:property name="max_synergy_velocity" value="100000" />
	<xacro:property name="max_synergy_effort" value="100" />
	<xacro:property name="synergy_damping" value="0.0" />
	<xacro:property name="synergy_friction" value="0.0" />

	<!-- SOFT FINGER model -->
	<!-- 4 fingers are equal (index, middle, ring, little) -->
	<xacro:macro name="soft_finger" params="parent parent_name name position orientation left useMimicTag abd_R inner_R middle_R outer_R">

		<!-- KNUCKLE -->
		<joint name="${name}_abd_joint" type="revolute">
			<origin xyz="${position}" rpy="${orientation}" />
			<parent link="${parent}"/>
			<child link="${name}_knuckle_link"/>
			<xacro:if value="${left}">
				<axis xyz="0 0 -1" />
			</xacro:if>
			<xacro:unless value="${left}">
				<axis xyz="0 0 1" />
			</xacro:unless>
			<limit lower="${abd_lb *pi/180}" upper="${abd_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
			<dynamics damping="${damping}" friction="${friction}"/>
			<xacro:if value="${useMimicTag}">
				<mimic joint="${parent_name}_synergy_joint" multiplier="${abd_R}"/>
			</xacro:if>
		</joint>
		
		<link name="${name}_knuckle_link">
			<inertial>
				<xacro:insert_block name="knuckle_inertia"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/knuckle.stl" scale=" .001 .001 .001" />
				</geometry>
				<material name="SoftHand/DarkGrey" />
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/knuckle_collision.stl" scale=" .001 .001 .001" />
				</geometry>
			</collision>
		</link>

		<!-- PROXIMAL PHALANGE -->
		<joint name="${name}_inner_joint" type="revolute">
			<origin xyz="0.009 0 0" rpy="0 0 0" />
			<parent link="${name}_knuckle_link"/>
			<child link="${name}_fake_link1"/>
			<axis xyz="0 -1 0" />
			<limit lower="${inner_lb *pi/180}" upper="${inner_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
			<dynamics damping="${damping}" friction="${friction}"/>
			<xacro:if value="${useMimicTag}">
				<mimic joint="${parent_name}_synergy_joint" multiplier="${inner_R}"/>
			</xacro:if>
		</joint>

		<link name="${name}_fake_link1">
			<inertial>
				<xacro:insert_block name="negligible_inertia"/>
			</inertial>
		</link>

		<joint name="${name}_inner_joint_mimic" type="revolute">
			<origin xyz="0.013 0 0" rpy="0 0 0" />
			<parent link="${name}_fake_link1"/>
			<child link="${name}_proximal_link"/>
			<axis xyz="0 -1 0" />
			<limit lower="${inner_lb *pi/180}" upper="${inner_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
			<dynamics damping="${damping}" friction="${friction}"/>
			<xacro:if value="${useMimicTag}">
				<mimic joint="${parent_name}_synergy_joint" multiplier="${inner_R}" />
			</xacro:if>
		</joint>

		<link name="${name}_proximal_link">
			<inertial>
				<xacro:insert_block name="phalanx_inertia"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/phalanx.stl" scale=" .001 .001 .001" />
				</geometry>
				<material name="SoftHand/DarkGrey" />
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/phalanx_collision.stl" scale=" .001 .001 .001" />
				</geometry>
			</collision>
		</link>

		<!-- MIDDLE PHALANGE -->
		<joint name="${name}_middle_joint" type="revolute">
			<origin xyz="0.009 0 0" rpy="0 0 0" />
			<parent link="${name}_proximal_link"/>
			<child link="${name}_fake_link2"/>
			<axis xyz="0 -1 0" />
			<limit lower="${middle_lb *pi/180}" upper="${middle_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
			<dynamics damping="${damping}" friction="${friction}"/>
			<xacro:if value="${useMimicTag}">
				<mimic joint="${parent_name}_synergy_joint" multiplier="${middle_R}"/>
			</xacro:if>
		</joint>

		<link name="${name}_fake_link2">
			<inertial>
				<xacro:insert_block name="negligible_inertia"/>
			</inertial>
		</link>

		<joint name="${name}_middle_joint_mimic" type="revolute">
			<origin xyz="0.013 0 0" rpy="0 0 0" />
			<parent link="${name}_fake_link2"/>
			<child link="${name}_middle_link"/>
			<axis xyz="0 -1 0" />
			<limit lower="${middle_lb *pi/180}" upper="${middle_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
			<dynamics damping="${damping}" friction="${friction}"/>
			<xacro:if value="${useMimicTag}">
				<mimic joint="${parent_name}_synergy_joint" multiplier="${middle_R}"/>
			</xacro:if>
		</joint>
		<link name="${name}_middle_link">
			<inertial>
				<xacro:insert_block name="phalanx_inertia"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/phalanx.stl" scale=" .001 .001 .001" />
				</geometry>
				<material name="SoftHand/DarkGrey" />
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/phalanx_collision.stl" scale=" .001 .001 .001" />
				</geometry>
			</collision>
		</link>

		<!-- DISTAL PHALANGE -->
		<joint name="${name}_outer_joint" type="revolute">
			<origin xyz="0.009 0 0" rpy="0 0 0" />
			<parent link="${name}_middle_link"/>
			<child link="${name}_fake_link3"/>
			<axis xyz="0 -1 0" />
			<limit lower="${outer_lb *pi/180}" upper="${outer_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
			<dynamics damping="${damping}" friction="${friction}"/>
			<xacro:if value="${useMimicTag}">
				<mimic joint="${parent_name}_synergy_joint" multiplier="${outer_R}"/>
			</xacro:if>
		</joint>

		<link name="${name}_fake_link3">
			<inertial>
				<xacro:insert_block name="negligible_inertia"/>
			</inertial>
		</link>

		<joint name="${name}_outer_joint_mimic" type="revolute">
			<origin xyz="0.013 0 0" rpy="0 0 0" />
			<parent link="${name}_fake_link3"/>
			<child link="${name}_distal_link"/>
			<axis xyz="0 -1 0" />
			<limit lower="${outer_lb *pi/180}" upper="${outer_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
			<dynamics damping="${damping}" friction="${friction}"/>
			<xacro:if value="${useMimicTag}">
				<mimic joint="${parent_name}_synergy_joint" multiplier="${outer_R}"/>
			</xacro:if>
		</joint>
		<link name="${name}_distal_link">
			<inertial>
				<xacro:insert_block name="fingertip_inertia"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/fingertip.stl" scale=" .001 .001 .001" />
				</geometry>
				<material name="SoftHand/DarkGrey" />
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/fingertip_collision.stl" scale=" .001 .001 .001" />
				</geometry>
			</collision>
		</link>

	</xacro:macro>

	<!-- THUMB MODEL -->
	<!-- It has one joint less than rest of fingers -->
	<xacro:macro name="soft_thumb" params="parent parent_name name left position orientation useMimicTag abd_R inner_R outer_R">

		<xacro:if value="${left}">
			<!-- KNUCKLE -->
			<joint name="${name}_abd_joint" type="revolute">
				<origin xyz="${position}" rpy="${orientation}" />
				<parent link="${parent}"/>
				<child link="${name}_knuckle_link"/>
				<axis xyz="0 0 1" />
				<limit lower="${thumb_abd_lb *pi/180}" upper="${thumb_abd_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
				<dynamics damping="${damping}" friction="${friction}"/>
				<xacro:if value="${useMimicTag}">
					<mimic joint="${parent_name}_synergy_joint" multiplier="${abd_R}"/>
				</xacro:if>
			</joint>
			
			<link name="${name}_knuckle_link">
				<inertial>
					<xacro:insert_block name="thumb_knuckle_inertia"/>
				</inertial>
				<visual>
					<geometry>
						<mesh filename="package://soft_hand_description/meshes/thumb_knuckle_left.stl" scale=" .001 .001 .001" />
					</geometry>
					<material name="SoftHand/DarkGrey" />
				</visual>
				<collision>
					<geometry>
						<mesh filename="package://soft_hand_description/meshes/thumb_knuckle_left_collision.stl" scale=" .001 .001 .001" />
					</geometry>
				</collision>
			</link>

			<!-- PROXIMAL PHALANGE -->
			<joint name="${name}_inner_joint" type="revolute">
				<origin xyz="0.022500000 0.003300000 0.0" rpy="-0.426627741408745 0 0.0" />
				<parent link="${name}_knuckle_link"/>
				<child link="${name}_fake_link1"/>
				<axis xyz="0 -1 0" />
				<limit lower="${inner_lb *pi/180}" upper="${inner_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
				<dynamics damping="${damping}" friction="${friction}"/>
				<xacro:if value="${useMimicTag}">
					<mimic joint="${parent_name}_synergy_joint" multiplier="${inner_R}"/>
				</xacro:if>
			</joint>
		</xacro:if>
		<xacro:unless value="${left}">
			<!-- KNUCKLE -->
			<joint name="${name}_abd_joint" type="revolute">
				<origin xyz="${position}" rpy="${orientation}" />
				<parent link="${parent}"/>
				<child link="${name}_knuckle_link"/>
				<axis xyz="0 0 -1" />
				<limit lower="${thumb_abd_lb *pi/180}" upper="${thumb_abd_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
				<dynamics damping="${damping}" friction="${friction}"/>
				<xacro:if value="${useMimicTag}">
					<mimic joint="${parent_name}_synergy_joint" multiplier="${abd_R}"/>
				</xacro:if>
			</joint>
			
			<link name="${name}_knuckle_link">
				<inertial>
					<xacro:insert_block name="thumb_knuckle_inertia"/>
				</inertial>
				<visual>
					<geometry>
						<mesh filename="package://soft_hand_description/meshes/thumb_knuckle_right.stl" scale=" .001 .001 .001" />
					</geometry>
					<material name="SoftHand/DarkGrey" />
				</visual>
				<collision>
					<geometry>
						<mesh filename="package://soft_hand_description/meshes/thumb_knuckle_right_collision.stl" scale=" .001 .001 .001" />
					</geometry>
				</collision>
			</link>

			<!-- PROXIMAL PHALANGE -->
			<joint name="${name}_inner_joint" type="revolute">
				<!-- <origin xyz="0.022500000 -0.003300000 0.0" rpy="0.42662757656587236 0.0 0.18131960430075023" /> -->
				<origin xyz="0.022500000 -0.003300000 0.0" rpy="0.42662757656587236 0.0 0.0"/>
				<parent link="${name}_knuckle_link"/>
				<child link="${name}_fake_link1"/>
				<axis xyz="0 -1 0" />
				<limit lower="${inner_lb *pi/180}" upper="${inner_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
				<dynamics damping="${damping}" friction="${friction}"/>
				<xacro:if value="${useMimicTag}">
					<mimic joint="${parent_name}_synergy_joint" multiplier="${inner_R}"/>
				</xacro:if>
			</joint>
		</xacro:unless>

		<link name="${name}_fake_link1">
			<inertial>
				<xacro:insert_block name="negligible_inertia"/>
			</inertial>
		</link>

		<joint name="${name}_inner_joint_mimic" type="revolute">
			<origin xyz="0.013 0 0" rpy="0 0 0" />
			<parent link="${name}_fake_link1"/>
			<child link="${name}_proximal_link"/>
			<axis xyz="0 -1 0" />
			<limit lower="${inner_lb *pi/180}" upper="${inner_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
			<dynamics damping="${damping}" friction="${friction}"/>
			<xacro:if value="${useMimicTag}">
				<mimic joint="${parent_name}_synergy_joint" multiplier="${inner_R}" />
			</xacro:if>
		</joint>

		<link name="${name}_proximal_link">
			<inertial>
				<xacro:insert_block name="phalanx_inertia"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/phalanx.stl" scale=" .001 .001 .001" />
				</geometry>
				<material name="SoftHand/DarkGrey" />
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/phalanx_collision.stl" scale=" .001 .001 .001" />
				</geometry>
			</collision>
		</link>

		<!-- DISTAL PHALANGE -->
		<joint name="${name}_outer_joint" type="revolute">
			<origin xyz="0.009 0 0.0" rpy="0 0 0" />
			<parent link="${name}_proximal_link"/>
			<child link="${name}_fake_link3"/>
			<axis xyz="0 -1 0" />
			<limit lower="${outer_lb *pi/180}" upper="${outer_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
			<dynamics damping="${damping}" friction="${friction}"/>
			<xacro:if value="${useMimicTag}">
				<mimic joint="${parent_name}_synergy_joint" multiplier="${outer_R}"/>
			</xacro:if>
		</joint>

		<link name="${name}_fake_link3">
			<inertial>
				<xacro:insert_block name="negligible_inertia"/>
			</inertial>
		</link>

		<joint name="${name}_outer_joint_mimic" type="revolute">
			<origin xyz="0.013 0 0" rpy="0 0 0" />
			<parent link="${name}_fake_link3"/>
			<child link="${name}_distal_link"/>
			<axis xyz="0 -1 0" />
			<limit lower="${outer_lb *pi/180}" upper="${outer_ub *pi/180}" effort="${effort}" velocity="${velocity}"/>
			<dynamics damping="${damping}" friction="${friction}"/>
			<xacro:if value="${useMimicTag}">
				<mimic joint="${parent_name}_synergy_joint" multiplier="${outer_R}" />
			</xacro:if>
		</joint>
		<link name="${name}_distal_link">
			<inertial>
				<xacro:insert_block name="fingertip_inertia"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/fingertip.stl" scale=" .001 .001 .001" />
				</geometry>
				<material name="SoftHand/DarkGrey" />
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://soft_hand_description/meshes/fingertip_collision.stl" scale=" .001 .001 .001" />
				</geometry>
			</collision>
		</link>
	</xacro:macro>

	<!-- SOFT HAND MODEL -->
	<xacro:macro name="soft_hand_soma_panda" params="parent name withAdaptiveTransmission useMimicTag left *origin">

		<!-- PACMAN VERION ALREADY INCLUDES THE COUPLER, CLAMP AND BASE -->

		<!-- PANDA FLANGE -->
		<xacro:panda_flange parent="${parent}" name="${name}_panda_flange">
			<origin xyz="0.0425 0.0 0.038" rpy="0.0 0.0 1.5707"/>
		</xacro:panda_flange>

		<!-- SOFT HAND ROUND BASE -->
		<xacro:softhand_round_base parent="${name}_panda_flange" name="${name}_softhand_round_base">
			<origin xyz="0.0 -0.011 0.0" rpy="1.5707 0.0 0.0"/>
		</xacro:softhand_round_base>

		<!-- PALM -->
		<joint name="${name}_palm_joint" type="fixed">
			<xacro:if value="${left}">
				<origin xyz="0.002 -0.00695 0.0535" rpy="0 0 0"/>
			</xacro:if>
			<xacro:unless value="${left}">
				<origin xyz="-0.008 0.005 0.018" rpy="0 0 1.5707"/>
			</xacro:unless>
			<parent link="${name}_softhand_round_base" />
			<child link="${name}_palm_link" />
		</joint>

		<link name="${name}_palm_link">
			<inertial>
				<xacro:insert_block name="palm_inertia"/>
			</inertial>
			<xacro:if value="${left}">
				<visual>
					<geometry>
						<mesh filename="package://soft_hand_description/meshes/palm_left.stl" scale=" .001 .001 .001" />
					</geometry>
					<material name="SoftHand/DarkGrey" />
				</visual>
				<collision>
					<geometry>
						<mesh filename="package://soft_hand_description/meshes/palm_left_collision.stl" scale=" .001 .001 .001" />
					</geometry>
				</collision>
			</xacro:if>
			<xacro:unless value="${left}">
				<visual>
					<geometry>
						<mesh filename="package://soft_hand_description/meshes/palm_right.stl" scale=" .001 .001 .001" />
					</geometry>
					<material name="SoftHand/DarkGrey" />
				</visual>
				<collision>
					<geometry>
						<mesh filename="package://soft_hand_description/meshes/palm_right_collision.stl" scale=" .001 .001 .001"/>
					</geometry>
				</collision>
			</xacro:unless>
		</link>

		<!-- MOTOR JOINT -->
		<joint name="${name}_synergy_joint" type="revolute" >
			<parent link="${name}_palm_link" />
			<child link="${name}_invisible_wire_link"/>
			<axis xyz="0 0 1" />
			<limit lower="${synergy_lb}" upper="${synergy_ub}" effort="${max_synergy_effort}" velocity="${max_synergy_velocity}"/>
			<dynamics damping="${synergy_damping}" friction="${synergy_friction}"/>
		</joint>
		<link name="${name}_invisible_wire_link">
			<inertial>
				<xacro:insert_block name="negligible_inertia"/>
			</inertial>
		</link>

		<!-- THUMB AND FINGERS -->
		<!-- change anchor points accordingly to the leftness -->
		<xacro:if value="${left}"> 
